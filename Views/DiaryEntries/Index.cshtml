@model List<DiaryEntry>

<div class = "table-responsive">
    <table class = "table table-striped table-hover">
        <thead>
            <tr>
                <th> Title </th>
                <th> Content </th>
                <th> Date </th>
                <th> Edit </th>
            </tr>
        </thead>
        <tbody>
            @foreach(var entry in Model)
            {
                <tr>
                    <td>@entry.Title</td>
                    <td>@entry.Content</td>
                    <td>@entry.CreatedAt</td>
                    <td>Edit or delete!</td>
                </tr>
            }
        </tbody>
    </table>
</div>